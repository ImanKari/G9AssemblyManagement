<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
	  <Version>1.0.0.0</Version>
	  <Authors>Iman Kari</Authors>
	  <Company>G9Studio</Company>
	  <Product>G9 Assembly Management</Product>
	  <Description>Database Version Control</Description>
	  <Copyright>G9Studio - 2021</Copyright>
	  <PackageProjectUrl>http://www.G9TM.com</PackageProjectUrl>
	  <RepositoryUrl>https://github.com/ImanKari/G9AssemblyManagement</RepositoryUrl>
	  <RepositoryType>GitHub</RepositoryType>
	  <PackageTags>Assembly Management dependency injection attribute</PackageTags>
	  <PackageReleaseNotes>-- Added: Assembly handler core
-- Added: Method to get inherited types from a type like interface or class (Any type like generic)
-- Added: Programming comments
-- Added: NUnit test</PackageReleaseNotes>
	  <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	  <AssemblyVersion>1.0.0.0</AssemblyVersion>
	  <FileVersion>1.0.0.0</FileVersion>
	  <ApplicationIcon></ApplicationIcon>
	  <PackageIcon>G9-Icon.png</PackageIcon>
	  <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
	  <PackageIconUrl />
	  <GenerateDocumentationFile>true</GenerateDocumentationFile>
	  <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
	  <RunPostBuildEvent>OnBuildSuccess</RunPostBuildEvent>
  </PropertyGroup>

  <ItemGroup>
    <None Update="G9-Icon.png">
      <Pack>True</Pack>
    </None>
    <None Update="LICENSE.md">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </None>
    <None Include="G9-Icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Helper\" />
  </ItemGroup>

  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="Set &quot;NugetLocalTargetPath=I:\LocalNugetPackages\&quot;&#xD;&#xA;if exist &quot;%25NugetLocalTargetPath%25&quot; (&#xD;&#xA;  xcopy &quot;bin\$(ConfigurationName)\*.nupkg&quot; &quot;%25NugetLocalTargetPath%25&quot; /C /Y&#xD;&#xA;) else (&#xD;&#xA;  echo 'Post-build event command line' : warning -9: 'Nuget target path not exist!'&#xD;&#xA;)" />
  </Target>

</Project>
